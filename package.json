{
  "name": "Ice-Air",
  "version": "1.1.0",
  "description": "A Ice-Air Application",
  "productName": "Ice-Air",
  "author": "loumt",
  "license": "ISC",
  "appId": "f4b264e-a322-224d-5d2s-xc21gs42dsd2",
  "main": "./dist/electron/main.js",
  "scripts": {
    "dev": "node build/dev-runner.js",
    "build": "npm run build:clean && npm run build:main && npm run build:renderer",
    "build:main": "cross-env NODE_ENV=production webpack --progress --colors --config build/webpack.main.config.js",
    "build:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config build/webpack.renderer.config.js",
    "build:web": "cross-env BUILD_TARGET=web node build/packager.js",
    "build:clean": "cross-env BUILD_TARGET=clean-electron node build/packager.js",
    "pack": "npm run pack:win32:x64 && npm run pack:win32:ia32",
    "pack:win32:x64": "cross-env BUILD_TARGET=packager DEBUG=electron-packager PTF_TARGET=win32 ARCH=x64 node build/packager.js",
    "pack:win32:ia32": "cross-env BUILD_TARGET=packager DEBUG=electron-packager PTF_TARGET=win32 ARCH=ia32 node build/packager.js",
    "pack:clean": "cross-env BUILD_TARGET=clean-packager node build/packager.js",
    "pack:asar:x64": "npm run pack:win32:x64 && npm run asar:x64",
    "builder": "electron-builder --config electron-builder.json",
    "asar:ia32": "cross-env ARCH=ia32 node build/asar.js",
    "asar:x64": "cross-env ARCH=x64 node build/asar.js",
    "asar": "cross-env WEB_URL=http://www.baidu.com electron ./dist/asar/client.asar"
  },
  "dependencies": {
    "async": "^2.6.1",
    "axios": "^0.18.0",
    "commander": "^2.16.0",
    "del": "^3.0.0",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "lodash.defaults": "^4.2.0",
    "log4js": "^2.10.0",
    "md5": "^2.2.1",
    "nconf": "^0.10.0",
    "node-rdp": "^1.0.1",
    "nprogress": "^0.2.0",
    "q": "^1.5.1",
    "sanitize-filename": "^1.6.1",
    "ssh2": "^0.6.1",
    "win-mouse": "^1.1.2"
  },
  "devDependencies": {
    "vue": "^2.5.2",
    "vue-electron": "^1.0.6",
    "vue-i18n": "^8.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "asar": "^0.14.3",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^2.1.3",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "electron": "^3.0.6",
    "electron-builder": "^20.29.0",
    "electron-debug": "^2.0.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-is-dev": "^0.3.0",
    "electron-localshortcut": "^3.1.0",
    "electron-log": "^2.2.16",
    "electron-packager": "^12.2.0",
    "electron-rebuild": "^1.8.2",
    "electron-settings": "^3.2.0",
    "element-ui": "^2.4.5",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.4",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-notifier": "^5.2.1",
    "portfinder": "^1.0.13",
    "postcss-loader": "^2.1.5",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.10",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.1.3"
  }
}
